{
	"name": "virty-api",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspaces/api/",
	"postCreateCommand": "git config --global --add safe.directory /workspaces",
	"customizations": {
		// Configure properties specific to VS Code.
		"vscode": {
			"extensions": [
				"ms-python.python",
				"mhutchie.git-graph"
			]
		}
	},
	"forwardPorts": [
		8765
	]
}