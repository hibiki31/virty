{
	"name": "WEB",
	// Dockerのビルド場所
	"context": "..",
	// ビルドするファイル、このjsonからの相対パス
	"dockerFile": "../Dockerfile.dev",
	"mounts": [
		// node_moduleは速くないと重いし残ってて欲しいから別マウント
		"source=virty-develop-node-modules,target=${containerWorkspaceFolder}/node_modules,type=volume",
	],
	"extensions": [
		"octref.vetur",
		"dbaeumer.vscode-eslint"
	],
	"forwardPorts": [
	],
}